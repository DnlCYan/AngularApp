{
  "name": "angular-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "gh-build": "ng build -c production --base-href=/AngularApp/",
    "gh-deploy": "npx gh-pages -d dist/angular-app",
    "gh-build-deploy": "npm run gh-build && npm run gh-deploy",
    "dev:ssr": "ng run AngularApp:serve-ssr",
    "serve:ssr": "node dist/AngularApp/server/main.js --ssl",
    "build:ssr": "ng build && ng run AngularApp:server",
    "prerender": "ng run AngularApp:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.4",
    "@angular/cdk": "^14.0.4",
    "@angular/common": "^14.0.4",
    "@angular/compiler": "^14.0.4",
    "@angular/core": "^14.0.4",
    "@angular/forms": "^14.0.4",
    "@angular/platform-browser": "^14.0.4",
    "@angular/platform-browser-dynamic": "^14.0.4",
    "@angular/platform-server": "^14.0.4",
    "@angular/router": "^14.0.4",
    "@angular/service-worker": "^14.0.4",
    "@nguniversal/express-engine": "^14.0.3",
    "express": "^4.15.2",
    "https": "^1.0.0",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.4",
    "@angular/cli": "~14.0.4",
    "@angular/compiler-cli": "^14.0.4",
    "@nguniversal/builders": "^14.0.3",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.3",
    "@types/node": "^14.15.0",
    "jasmine-core": "~4.1.1",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.4"
  }
}
